public class Day75 {
    public static void main(String[] args) {
        
        String kapital = "JUHARIANA";
        
        String huruf_kecil = kapital.toLowerCase();
        
        System.out.println("nama saya dalam huruf kapital : " + kapital);//JUHARIANA
        System.out.println("nama saya dalam huruf kecil : " + huruf_kecil);//juhariana
    }
}public class Day75 {
